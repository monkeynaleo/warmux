<!-- ###### Jump ###### -->
<movement name="jump" speed="100">
	<collision_rect left="5" right="5" top="10" bottom="2"/>
	<frame number="0">
		<member type="head" follow_half_crosshair="false"/> <!-- follow_half_crosshair : the head rotates when we move the crosshair -->
		<member type="arm-left-1" angle="90" follow_speed="false"/> <!--follow_speed : rotates in the direction where the character is going -->
		<member type="arm-right-1" angle="90" follow_speed="false"/>
		<member type="foot-left" angle="0" follow_speed="true"/>
		<member type="foot-right" angle="0" follow_speed="true"/>
	</frame>
</movement>

<!-- ###### Fall ###### -->
<movement name="fall" speed="100">
	<collision_rect left="5" right="5" top="10" bottom="2"/>
	<frame number="0">
		<member type="head" follow_half_crosshair="false"/> <!-- follow_half_crosshair : the head rotates when we move the crosshair -->
		<member type="arm-left-1" angle="-90" follow_speed="true"/> <!--follow_speed : rotates in the direction where the character is going -->
		<member type="arm-right-1" angle="-90" follow_speed="true"/>
		<member type="foot-left" angle="0" follow_speed="true"/>
		<member type="foot-right" angle="0" follow_speed="true"/>
	</frame>
</movement>

