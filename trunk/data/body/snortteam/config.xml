<!-- ###### Body ###### -->
<sprite name="body-normal" type="body">
	<image file="body.png"/>
	<anchor dx="16" dy="17"/>
	<attached member_type="head" frame="*" dx="5" dy="7"/>
	<attached member_type="arm-right" frame="*" dx="5" dy="17"/>
	<attached member_type="arm-left" frame="*" dx="10" dy="16"/>
	<attached member_type="foot-left" frame="*" dx="13" dy="30"/>
	<attached member_type="foot-right" frame="*" dx="7" dy="31"/>
	<attached member_type="jetpack-belt" frame="*" dx="1" dy="22"/>
	<attached member_type="jetpack-right" frame="*" dx="12" dy="32"/>
	<attached member_type="jetpack-left" frame="*" dx="12" dy="31"/>
	<attached member_type="tail" frame="*" dx="3" dy="31"/>
</sprite>


<!-- ###### Tail ###### -->
<sprite name="tail" type="tail">
	<image file="tail.png"/>
	<anchor dx="8" dy="2"/>
</sprite>

<!-- ###### Head ###### -->
<sprite name="head" type="head">
	<image file="head.png"/>
	<anchor dx="7" dy="11"/>
	<attached member_type="helmet" frame="*" dx="7" dy="10"/>
</sprite>

<!-- ###### Arms ###### -->
<sprite name="arm-left" type="arm-left">
	<image file="arm.png"/>
	<anchor dx="3" dy="0"/>
</sprite>

<sprite name="arm-right" type="arm-right">
	<image file="arm.png"/>
	<anchor dx="3" dy="0"/>
	<attached member_type="weapon" frame="*" dx="3" dy="10"/>
	<attached member_type="yoyo" frame="*" dx="3" dy="8"/>
</sprite>

<!-- ###### Legs ###### -->
<sprite name="foot-left" type="foot-left" on_ground="true">
	<image file="arm.png"/>
	<anchor dx="2" dy="0"/>
</sprite>

<sprite name="foot-right" type="foot-right" on_ground="true">
	<image file="arm.png"/>
	<anchor dx="2" dy="0"/>
</sprite>

<!-- ###### Jetpack ###### -->
<sprite name="jetpack-belt" type="jetpack-belt" on_ground="true">
	<image file="jetpack-belt.png"/>
	<anchor dx="0" dy="0"/>
</sprite>

<sprite name="jetpack-right" type="jetpack-right" on_ground="true">
	<image file="jetpack.png"/>
	<anchor dx="4" dy="10"/>
	<attached member_type="jetpack-fire-right" frame="*" dx="4" dy="3"/>
</sprite>

<sprite name="jetpack-left" type="jetpack-left" on_ground="true">
	<image file="jetpack.png"/>
	<anchor dx="4" dy="10"/>
	<attached member_type="jetpack-fire-left" frame="*" dx="4" dy="3"/>
</sprite>

<sprite name="jetpack-fire-right" type="jetpack-fire-right" on_ground="true">
	<image file="jetpack-fire.png"/>
	<anchor dx="2" dy="0"/>
</sprite>

<sprite name="jetpack-fire-left" type="jetpack-fire-left" on_ground="true">
	<image file="jetpack-fire.png"/>
	<anchor dx="2" dy="0"/>

</sprite>

<!-- ###### Helmet ###### -->
<sprite name="helmet" type="helmet" on_ground="true">
	<image file="helmet.png"/>
	<anchor dx="9" dy="13"/>
</sprite>

<!-- ###### Yoyo ###### -->
<sprite name="yoyo" type="yoyo" go_through_ground="true">
